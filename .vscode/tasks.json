{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "dev watch",
			"type": "shell",
			"command": "./node_modules/.bin/tsc",
			"args": [
				"--project",
				"./",
				"--sourceMap",
				"--watch"
			],
			"problemMatcher": [
				"$tsc-watch"
			],
			"isBackground": true,
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}